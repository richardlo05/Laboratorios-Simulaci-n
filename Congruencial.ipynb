    "def congruencial(a,b,m,n,Xo):\n",
    "    vector=[]\n",
    "    for i in range(n):\n",
    "        Xn=(a*Xo+b)%m\n",
    "        Xo=Xn\n",
    "        Un=Xn/float(m)\n",
    "        vector.append(Un)\n",
    "    return vector\n",
    "v = congruencial(34,32,34,32,23) \n",
    "print (v)"
